<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="/stylesheets/alldetails.css">
  </head>
  <body>
    <!-- bootstrap start -->
    <nav class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
          <a class="logo" href="/login">Padget</a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                  Plant
                </a>
                <ul class="dropdown-menu">
                  <li><a class="dropdown-item" href="">Sector-58</a></li>
                  <li><a class="dropdown-item" href="">Sector-60</a></li>
                  <li><a class="dropdown-item" href="">Sector-63</a></li>
                  <li><a class="dropdown-item" href="/info/plant/abc@gmail.com">Sector-68</a></li>
                </ul>
              </li>
               <!-- second dropdown -->
              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                  Project
                </a>
                <ul class="dropdown-menu">
                  <li><a class="dropdown-item" href="/info/projectDetails/Xiaomi">Xiaomi</a></li>
                  <li><a class="dropdown-item" href="/info/projectDetails/Compal">Compal</a></li>
                  <li><a class="dropdown-item" href="/info/projectDetails/Lenovo">Lenovo</a></li>
                  <li><a class="dropdown-item" href="/info/projectDetails/Moto">Moto-SMT</a></li>
                  <li><a class="dropdown-item" href="/info/projectDetails/Jio">Jio</a></li>
                  <li><a class="dropdown-item" href="/info/projectDetails/Acer">Acer</a></li>
                </ul>
              </li>
              

              <li class="nav-item dropdown">
                <a class="nav-link" href="/info/downloadCsv">
                  Download
                </a>
              </li>
            </ul>
            <a href="/login" class="d-flex btn btn-outline-primary">Login</a>
          </div>
        </div>
      </nav>
      <!-- bootstrap end -->
       <div class="heading">
        <h1>All Details</h1>
       </div>
       <div class="boxs">
        <% allInfo.forEach((val)=>{ %>
            <div class="box">
                <div class="info">
                    <h5>Section : <%= val.section %></h5>
                    <h5>Model : <%= val.model %></h5>
                    <h5>Std_M.P : <%= val.stdMp %></h5>
                    <h5>Actual_M.P : <%= val.actualMp %></h5>
                    <h5>Std_UPH : <%= val.stdUph %></h5>
                    <h5>Actual_UPH : <%= val.actualUph %></h5>
                    <h5>Project: <%= val.project %></h5>
                </div>
            </div>
        <% }) %>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
  </body>
</html>