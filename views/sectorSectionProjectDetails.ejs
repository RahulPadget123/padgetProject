<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="/stylesheets/projectDetails.css">
  </head>
  <body>
    <!-- bootstrap start -->
    <nav class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
          <a class="logo" href="/login">Padget</a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
              <!-- <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                  Section
                </a>
                <ul class="dropdown-menu">
                  <li><a class="dropdown-item" href="">SMT</a></li>
                  <li><a class="dropdown-item" href="">BE</a></li>
                  <li><a class="dropdown-item" href="">CFC</a></li>
                </ul>
              </li> -->
              
              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                  Model
                </a>
                <!-- model-option -->
                 <% if(projectName === "Xiaomi"){ %>
                  <ul class="dropdown-menu">
                    <li><a class="dropdown-item" href="/info/modelDetails/C3F/<%= sectionName %>/<%= projectName %>/<%= userid %>">C3F</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/C3FP/<%= sectionName %>/<%= projectName %>/<%= userid %>">C3FP</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/C3F2P/<%= sectionName %>/<%= projectName %>/<%= userid %>">C3F2P</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/C3Y2/<%= sectionName %>/<%= projectName %>/<%= userid %>">C3Y2</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/C3ZP/<%= sectionName %>/<%= projectName %>/<%= userid %>">C3ZP</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/C3YP/<%= sectionName %>/<%= projectName %>/<%= userid %>">C3YP</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/C3Z/<%= sectionName %>/<%= projectName %>/<%= userid %>">C3Z</a></li>
                  </ul>
                 <% } else if(projectName === "Moto"){ %>
                  <ul class="dropdown-menu">
                    <li><a class="dropdown-item" href="/info/modelDetails/Malmo/<%= sectionName %>/<%= projectName %>/<%= userid %>">Malmo</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/Vegas/<%= sectionName %>/<%= projectName %>/<%= userid %>">Vegas</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/Manila/<%= sectionName %>/<%= projectName %>/<%= userid %>">Manila</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/Kansas/<%= sectionName %>/<%= projectName %>/<%= userid %>">Kansas</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/Fogo/<%= sectionName %>/<%= projectName %>/<%= userid %>">Fogo</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/Milos/<%= sectionName %>/<%= projectName %>/<%= userid %>">Milos</a></li>
                    <li><a class="dropdown-item" href="/info/modelDetails/Casco 5G/<%= sectionName %>/<%= projectName %>/<%= userid %>">Casco 5G</a></li>
                  </ul>
                 <% } else if(projectName === "Lenovo"){ %>
                  <ul class="dropdown-menu">
                    <li><a class="dropdown-item" href="/info/modelDetails/E-14/<%= sectionName %>/<%= projectName %>/<%= userid %>">E-14</a></li>
                  </ul>
                 <% } %>
              </li>
            </ul>
            <a href="/info/allDetails" class="d-flex btn btn-outline-primary">Home</a>
          </div>
        </div>
      </nav>
      <!-- bootstrap end -->
       <div class="heading">
        <h1>Plant-Project(<%= projectName %>)-Section(<%= sectionName %>)</h1>
       </div>
       <div class="boxs">
        <% info.forEach((val)=>{ %>
          <div class="box">
              <div class="info">
                  <h5>Section : <%= val.section %></h5>
                  <h5>Model : <%= val.model %></h5>
                  <h5>Std_M.P : <%= val.stdMp %></h5>
                  <h5>Actual_M.P : <%= val.actualMp %></h5>
                  <h5>Std_UPH : <%= val.stdUph %></h5>
                  <h5>Actual_UPH: <%= val.actualUph %></h5>
                  <h5>Project: <%= val.project %></h5>
              </div>
          </div>
      <% }) %>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
  </body>
</html>